{
	"name": "inlinement",
	"displayName": "inlinement",
	"description": "VSC Extension to align inline comments by their leading '//'",
	"version": "0.0.1",
	"publisher": "NerdyDeedsLLC",
	"icon": "icon/icon.png",
	"repository": {
		"type": "git",
		"url":"https://github.com/NerdyDeedsLLC/vscode-inlinement.git"
	},
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.inlinement"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
            "command": "extension.inlinement",
			"title": "Inlinement: Align by Rightmost Inline Comment"
		}]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"vscode": "^1.1.25",
		"eslint": "^4.11.0"
	}
}
