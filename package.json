{
	"name": "inlinement",
	"displayName": "inlinement",
	"description": "VSC Extension to align inline comments by their leading '//'",
	"version": "0.0.1",
	"publisher": "NerdyDeedsLLC",
	"icon": "icon/icon128.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/NerdyDeedsLLC/vscode-inlinement.git"
	},
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:inlinement"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "inlinement",
				"title": "Inlinement: Align Comments"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "inlinement",
					"when": "never"
				}
			]
		}
	},
	"scripts": {
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"eslint": "^4.11.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}